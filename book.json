{
    "plugins": ["-lunr", "-search", "search-plus", "include-codeblock", "ace", "theme-api"],
    "pluginsConfig": {
        "theme-api": {
            "theme": "dark",
            "split": true,
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "include-codeblock": {
            "template": "acefull",
            "unindent": true,
            "theme": "tomorrow"
        }
    }
}
